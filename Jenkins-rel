pipeline {
    agent { label'TRF' }
    stages {
        stage ('clone') {
            steps {
                git url: 'https://github.com/chandushine/Calculator-1.git',
                 branch: 'rel'
            }
        }   
        stage ('merge') {
            steps {
                sh 'git merge --no-ff dev'
            }    
        stage ('build') {
            steps {
                sh 'mvn clean install'
            }
        }
          
    }
    }